{
  "name": "portfolio",
  "private": true,
  "version": "1.0.4",
  "description": "Modern React TypeScript portfolio website",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tihomirkg91/portfolio"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "vite",
    "frontend"
  ],
  "author": "Tihomir Tomovic",
  "license": "MIT",
  "sideEffects": false,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check-circular": "madge --circular --extensions ts,tsx src/",
    "reset": "rm -rf node_modules && npm cache clean --force && npm install",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@types/pdfmake": "^0.2.11",
    "moment": "^2.30.1",
    "pdfmake": "^0.2.20",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  }
}
