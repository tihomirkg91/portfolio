{
  "name": "portfolio",
  "private": true,
  "version": "2.0.3",
  "description": "Modern React TypeScript portfolio website",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tihomirkg91/portfolio"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "vite",
    "frontend"
  ],
  "author": "Tihomir Tomovic",
  "license": "MIT",
  "sideEffects": false,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "check-circular": "madge --circular --extensions ts,tsx src/",
    "reset": "rm -rf node_modules && npm cache clean --force && npm install",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@types/pdfmake": "^0.2.12",
    "moment": "^2.30.1",
    "pdfmake": "^0.2.20",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  }
}
